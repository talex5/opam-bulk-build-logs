/usr/bin/mpicc.openmpi -I`ocamlc -where` -I/usr/lib/openmpi/include -O2 -g -Wall   -c -o groups.o groups.c
/usr/bin/mpicc.openmpi -I`ocamlc -where` -I/usr/lib/openmpi/include -O2 -g -Wall   -c -o utils.o utils.c
rm -f libcamlmpi.a
ar rc libcamlmpi.a init.o comm.o msgs.o collcomm.o groups.o utils.o
ocamlc -c mpi.mli
ocamlc -c mpi.ml
ocamlc -a -o mpi.cma -custom mpi.cmo -cclib -lcamlmpi -ccopt -L/usr/lib/openmpi/lib -cclib -lmpi
ocamlopt -c mpi.ml
ocamlopt -a -o mpi.cmxa mpi.cmx -cclib -lcamlmpi -ccopt -L/usr/lib/openmpi/lib -cclib -lmpi
ocamlfind install mpi META mpi.mli mpi.cmi mpi.cma mpi.cmxa libcamlmpi.a mpi.a
