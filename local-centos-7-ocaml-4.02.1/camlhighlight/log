- ./setup.exe -build 
- /usr/bin/ocamlopt.opt unix.cmxa -I /usr/lib64/ocaml/ocamlbuild /usr/lib64/ocaml/ocamlbuild/ocamlbuildlib.cmxa myocamlbuild.ml /usr/lib64/ocaml/ocamlbuild/ocamlbuild.cmx -o myocamlbuild
- /home/opam/.opam/system/bin/ocamlfind ocamldep -package str -modules lib/xml.mli > lib/xml.mli.depends
- /home/opam/.opam/system/bin/ocamlfind ocamlc -c -g -bin-annot -package str -I lib -o lib/xml.cmi lib/xml.mli
- /home/opam/.opam/system/bin/ocamlfind ocamldep -package str -modules lib/xml.ml > lib/xml.ml.depends
